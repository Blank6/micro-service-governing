<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.springcloud.service.governing.dao.UserDao">

    <select id="getUser" resultType="org.springcloud.service.governing.entity.request.UserEntity">
        SELECT * FROM table_user;
    </select>

    <select id="getUserById" resultType="org.springcloud.service.governing.entity.request.UserEntity">
        SELECT * FROM table_user WHERE userId = #{userId};
    </select>

</mapper>